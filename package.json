{
  "name": "stockholm",
  "description": "Fully featured theme with clean presentation.",
  "homepage": "https://stockholm.ghostthemes.io/",
  "demo": "https://hackersandslackers.com/",
  "version": "0.0.1",
  "main": "src/js/global.js",
  "scripts": {
    "clean": "rm js/bundle.js",
    "develop": "webpack -d --mode development",
    "build": "webpack --mode production",
    "test": "NODE_ENV=/"
  },
  "presets": [
    "env"
  ],
  "author": {
    "name": "Todd Birchard",
    "email": "toddbirchard@gmail.com",
    "url": "https://toddbirchard.com"
  },
  "keywords": [
    "ghost-theme",
    "blog-theme",
    "ghost",
    "theme",
    "handlebars"
  ],
  "engines": {
    "node": ">=v12",
    "ghost": ">=3.11.0",
    "ghost-api": "v2"
  },
  "screenshots": {
    "desktop": "https://hackers-content.nyc3.digitaloceanspaces.com/images/social/share.jpg",
    "mobile": "https://hackers-content.nyc3.digitaloceanspaces.com/images/social/share.jpg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/toddbirchard/ghosttheme-stockholm"
  },
  "config": {
    "posts_per_page": 10,
    "image_sizes": {
      "small": {
        "width": 370
      },
      "small_retina": {
        "width": 840
      },
      "medium": {
        "width": 700
      },
      "medium_retina": {
        "width": 1400
      },
      "large": {
        "width": 780
      },
      "large_retina": {
        "width": 1560
      }
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "@babel/runtime": "^7.10.3",
    "@sendgrid/mail": "^7.2.0",
    "babel-register": "^6.26.0",
    "baguettebox.js": "^1.11.1",
    "dotenv-webpack": "^3.0.0",
    "es6-promise": "^4.2.8",
    "fuzzysort": "^1.1.4",
    "ghost-email-service": "^1.2.0",
    "ghost-pagination": "^0.1.3",
    "ghost-search": "^1.1.0",
    "github-profile-card": "^3.1.0",
    "highlight.js": "^10.4.1",
    "isomorphic-fetch": "^3.0.0",
    "jquery": "^3.5.1",
    "lazysizes": "^5.2.2",
    "scrollbooster": "^3.0.2",
    "slick-carousel": "^1.8.1",
    "vanilla-lazyload": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "babel-loader": "^8.1.0",
    "css-declaration-sorter": "5.1.2",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "font-config-webpack-plugin": "^2.0.0",
    "html-minifier-webpack-plugin": "^2.2.3",
    "less": "^3.11.3",
    "less-loader": "^6.1.2",
    "lodash": ">=4.17.19",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "terser-webpack-plugin": "^3.0.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
