<!-- start header -->
<nav class="main-navigation">
		{{navigation}}
</nav>
<header class="main-header videoHeader">

	<div class="headerContainer">
		<!-- start logo -->
		<a class="mobile-branding" href="{{@blog.url}}"><img src="https://hackers.nyc3.digitaloceanspaces.com/logowhite.png"></a>
	{{#if @blog.logo}}
		<a class="branding" href="{{@blog.url}}"><img src="{{@blog.logo}}" alt="{{@blog.title}}"></a>
	{{else}}
		<a class="branding" href="{{@blog.url}}">{{@blog.title}}</a>
	{{/if}}
	<!-- start navigation -->
</div>
	<video class="videoObject" autoplay muted loop id="myVideo">
		<source src="https://hackersandslackers.com/assets/vid/bg.mp4" type="video/mp4">
	</video>
</header>
